{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "components/Event_Dashboard_Nav_bar.tsx",
    "util/Toastify.js",
    "util/UserCheck.js",
    "app/api/v1/user/exist/route.ts",
    "app/auth/login/page.tsx",
    "components/Dashboard.tsx",
    "app/api/v1/createOrganization/route.js",
    "app/organization/dashboard/[id]/OrgContext.jsx",
    "app/admin/dashboard/[id]/AdminContext.jsx",
    "app/admin/dashboard/[id]/components/Notification.jsx"
  ],
  "exclude": ["node_modules"]
}
